<%- layout('layout/layout.ejs') %>

<%- include('paritials/topbar.ejs') %>
    <div id="viewscreen" >
        <%- include('paritials/sidenavbar.ejs') %>
        <div id="rightview" >
            <div class="container-fluid div-min-width" style="padding:0;">
                <div class="row">
                    <div class="col-lg-12 parentDiv" style="padding:0">
                        <div class="container">
                            <div class="col-sm-3"></div>
                            <div class="col-sm-6 mainDiv">
                                <br>
                                <div class="alert alert-info" id="tag_info" style="display: none;">
                                    <center id="display_email">adsaa</center>
                                </div>
                                   
                                <h1 class="heading">Create Tag</h1>
                                <form class="form-horizontal formDiv">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <input type="text" class="form-control" id="taging" placeholder="Enter Tag" onchange="checkTagAvail()">
                                            <br>
                                        </div>

                                        <div class="form-group">
                                            <input value="Submit" class="btn btn-success" id="submit-btn" readonly="">
                                            <input value="Show Tags" class="btn btn-primary" id="showtags" readonly="">
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

<script type="text/javascript" src="js/create_tag.js"></script>

</html>