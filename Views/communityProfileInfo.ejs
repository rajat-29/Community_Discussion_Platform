<%- layout('layout/layout.ejs') %> 

<%- include('partials/topbar.ejs') %>
        <div id="viewscreen" >
            <%- include('partials/sidenavbar.ejs') %>
  
            <div id="rightview">
                <div class="container-fluid" style="padding:0">
                    <div class="row">
                        <div class="col-lg-12" style="padding:0">
                            <div class="to-center-div">
                                <div class="container">
                                    <div class="panel panel-default" style="background: rgb(249, 249, 249);box-shadow: 0 3px 10px 0 rgba(115,143,147,.3);">
                                        <div class="panel-body">

                                            <div class="d-flex imageDiv">
                                                <div class="image-container">
                                                    <img src="<%= newdata.commphoto %>" id="profilepic">
                                                </div>
                                            </div>

                                            <br><br>

                                            <div class="col-sm-6">
                                                <div class="panel panel-default">
                                                    <div class="panel-heading">Name</div>
                                                    <div class="panel-body"><%=newdata.name%></div>
                                                </div>
                                            </div>

                                            <div class="col-sm-6">
                                                <div class="panel panel-default">
                                                    <div class="panel-heading">Description</div>
                                                    <div class="panel-body"><%=newdata.desc%></div>
                                                </div>
                                            </div>

                                            <div class="col-sm-6">
                                                <div class="panel panel-default">
                                                    <div class="panel-heading">Rule</div>
                                                    <div class="panel-body"><%=newdata.rule%></div>
                                                </div>
                                            </div>

                                            <div class="col-sm-6">
                                                <div class="panel panel-default">
                                                    <div class="panel-heading">Status</div>
                                                    <div class="panel-body"><%=newdata.status%></div>
                                                </div>
                                            </div>

                                            <div class="col-sm-6">
                                                <div class="panel panel-default">
                                                    <div class="panel-heading">Owner</div>
                                                    <div class="panel-body"><%=newdata.owner%></div>
                                                </div>
                                            </div>

                                            <div class="col-sm-6">
                                                <div class="panel panel-default">
                                                    <div class="panel-heading">Location</div>
                                                    <div class="panel-body"><%=newdata.location%></div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</html>