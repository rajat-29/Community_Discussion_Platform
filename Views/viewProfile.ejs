<%- layout('layout/layout.ejs') %> 

<%- include('paritials/topbar.ejs') %>
<div id="viewscreen" >
    <%- include('paritials/sidenavbar.ejs') %>
        <div id="rightview">
            <div class="container-fluid" style="padding:0">
                <div class="row">
                    <div class="col-lg-12 " style="padding:0">
                        <div class="to-center-div">
                            <div class="container childDiv">

                                <div class="panel panel-default" style="background: rgb(249, 249, 249);box-shadow: 0 3px 10px 0 rgba(115,143,147,.3);">
                                    <div class="panel-body">

                                        <div class="col-sm-6">
                                            <div class="panel panel-default panelDiv">
                                                <div class="panel-heading">Email</div>
                                                <div class="panel-body"><%=newdata.email%></div>
                                            </div>
                                        </div>

                                        <div class="col-sm-6">
                                            <div class="panel panel-default">
                                                <div class="panel-heading">Gender</div>
                                                <div class="panel-body"><%=newdata.gender%></div>
                                            </div>
                                        </div>

                                        <div class="col-sm-6">
                                            <div class="panel panel-default">
                                                <div class="panel-heading">Name</div>
                                                <div class="panel-body"><%=newdata.name%></div>
                                            </div>
                                        </div>

                                        <div class="col-sm-6">
                                            <div class="panel panel-default">
                                                <div class="panel-heading">City</div>
                                                <div class="panel-body"><%=newdata.city%></div>
                                            </div>
                                        </div>

                                        <div class="col-sm-6">
                                            <div class="panel panel-default">
                                                <div class="panel-heading">Dob</div>
                                                <div class="panel-body"><%=newdata.dob%></div>
                                            </div>
                                        </div>

                                        <div class="col-sm-6">
                                            <div class="panel panel-default">
                                                <div class="panel-heading">Phone</div>
                                                <div class="panel-body"><%=newdata.phone%></div>
                                            </div>
                                        </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</html>